@{
    ViewData["Title"] = "Privacy Policy";
}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<style>
    #aboutusbody {
    background-image: url(../../wwwroot/img/aboutusbg.jpg);
     }
    
    #mv {
    padding-top: 20px;
    }
    .carousel-inner img {
    width: 100%;
    height: 100%;
    padding-top: 70px;
    }
    </style>
<div id="demo" class="carousel slide" data-ride="carousel">

    <!-- Indicators -->
    <ul class="carousel-indicators">
        <li data-target="#demo" data-slide-to="0" class="active"></li>
        <li data-target="#demo" data-slide-to="1"></li>
        <li data-target="#demo" data-slide-to="2"></li>
    </ul>

    <!-- The slideshow -->
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/img/Carousel 1.1.png" alt="Los Angeles" width="1100" height="500">
        </div>
        <div class="carousel-item">
            <img src="~/img/Carousel 2.1.png" alt="Chicago" width="1100" height="500">
        </div>
        <div class="carousel-item">
            <img src="~/img/Carousel 3.png" alt="New York" width="1100" height="500">
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="carousel-control-prev" href="#demo" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </a>
    <a class="carousel-control-next" href="#demo" data-slide="next">
        <span class="carousel-control-next-icon"></span>
    </a>
</div>

<div class="jumbotron-fluid" style="background-color:azure">
    <div class="row">
        <div class="col col-6">
            <img src="~/img/5.jpg" style="position: center; width: 100%;" />
        </div>
        <div class="col col-6" style="text-align: center; padding-top: 150px; padding-left: 100px; padding-right: 100px;">
            <h1 style="color: darkblue">Vision</h1>
            <br />
            <h5 style="color: darkblue">
                To always provide an unparalleled experience as the most trusted partner
                for health care to every Filipino, every life - whole and healthy.
                We work to be trusted by patients, valued partners in the community, and creators of positive change.
            </h5>
        </div>
    </div>
</div>
<div class="jumbotron-fluid" style=" background-color: azure;" id="mv">
    <div class="row">
        <div class="col col-6" style="text-align: center;  padding-top: 150px; padding-left: 100px; padding-right: 100px;">
            <h1 style="color: darkblue">Mission</h1>
            <br />
            <h5 style="color: darkblue">
                Providing patient-centered healthcare that is exceptional
                in terms of quality, service, and accessibility.
            </h5>
        </div>
        <div class="col col-6">
            <img src="~/img/7.jpg" style="position: center; width: 100%;" />
        </div>
    </div>
</div>